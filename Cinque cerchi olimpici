<h3>Cinque cerchi olimpici</h3>
 <canvas id="olympicRingsCanvas" width="1340" height="615"></canvas> 
<script> 
const canvas = document.getElementById('olympicRingsCanvas'); 
const ctx = canvas.getContext('2d'); 
ctx.beginPath(); 
ctx.arc(75, 100, 50, 0, 2 * Math.PI); 
ctx.strokeStyle = 'blue'; 
ctx.lineWidth = 10;
 ctx.stroke(); 
 ctx.beginPath(); 
ctx.arc(150, 100, 50, 0, 2 * Math.PI);
 ctx.strokeStyle = 'black'; 
ctx.lineWidth = 10;
 ctx.stroke(); 
 ctx.beginPath();
 ctx.arc(225, 100, 50, 0, 2 * Math.PI); 
ctx.strokeStyle = 'red';
 ctx.lineWidth = 10; 
ctx.stroke(); 
 ctx.beginPath(); 
ctx.arc(110, 150, 50, 0, 2 * Math.PI);
 ctx.strokeStyle = 'yellow'; 
ctx.lineWidth = 10; ctx.stroke();
 ctx.beginPath(); 
ctx.arc(185, 150, 50, 0, 2 * Math.PI); 
ctx.strokeStyle = 'green'; 
ctx.lineWidth = 10; 
ctx.stroke();
 </script>
